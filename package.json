{
    "name": "futuremotion-midnight",
    "version": "1.0.0",
    "displayName": "Futuremotion Midnight Theme",
    "homepage": "https://github.com/futuremotiondev/vscode-futuremotion-midnight",
    "description": "An sleek and streamlined dark coding experience.",
    "publisher": "Futuremotion",
    "license": "LICENSE.md",
    "scripts": {
        "package": "mkdir bin && vsce package -o ./bin/futuremotion-midnight.vsix",
        "vsce-publish": "vsce publish"
    },
    "maintainers": [
        "Futuremotion"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/futuremotiondev/vscode-futuremotion-midnight.git"
    },
    "bugs": {
        "url": "https://github.com/futuremotiondev/vscode-futuremotion-midnight/issues"
    },
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "midnight",
        "dark",
        "theme",
        "color-theme"
    ],
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#17191D",
        "theme": "dark"
    },
    "contributes": {
        "themes": [
            {
                "label": "Futuremotion Midnight Theme",
                "uiTheme": "vs-dark",
                "path": "./theme/fm-midnight.json"
            }
        ]
    }
}
